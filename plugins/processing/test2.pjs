
var w    = 100;
var h    = 100;
float r = 0.0;

void setup() {
  size(width, height);
  
  // Frame rate
  frameRate(8);
}

void draw() {
  background(#ff0000);
  translate(width/2, height/2);
  for(int i = 0; i<r; i++){
	rotate(PI/4);
  }
  rect(-26, -26, 52, 52);

  r++;
  
  if(r == 5){
	r=0;
  }
}
